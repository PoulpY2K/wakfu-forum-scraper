generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model topic {
  topic_id Int    @id(map: "topic_pk") @default(autoincrement())
  name     String @unique @db.VarChar
  count    Int    @default(0)
}
